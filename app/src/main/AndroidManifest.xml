<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.duatson.studentapp">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_ctsv_app_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_ctsv_app_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:ignore="AllowBackup,GoogleAppIndexingWarning">
        <activity android:name=".RequestDetailActivity" />
        <activity android:name=".ServiceDetailActivity" />
        <activity android:name=".RegisterServiceActivity" />
        <activity android:name=".ProfileActivity" />
        <activity android:name=".ServiceListActivity" />
        <activity android:name=".fragment.DashboardFragment" />
        <activity android:name=".FeedbackActivity" />
        <activity
            android:name=".MainActivity"
            android:windowSoftInputMode="adjustPan">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>